{
  "name": "docs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "tokens:css": "node scripts/generate-color-css.mjs",
    "tokens:colors": "node scripts/generate-colors-ts.mjs",
    "tokens:foundation": "node scripts/generate-foundation-css.mjs",
    "check:raw-colors": "node scripts/check-no-raw-colors.mjs",
    "tokens": "npm run tokens:css && npm run tokens:colors && npm run tokens:foundation",
    "predev": "npm run tokens",
    "dev": "next dev",
    "prebuild": "npm run tokens && npm run check:raw-colors",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "deploy": "gh-pages -d out"
  },
  "dependencies": {
    "@baerae-zkap/design-system": "link:packages/design-system",
    "lucide-react-native": "^0.563.0",
    "next": "16.1.6",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "gh-pages": "^6.3.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
